<h1>Update your Calender</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@calender) do |f| %>

        <%= f.label :title %>
        <%= f.text_field :title, class: 'form-control' %>
        <%= f.submit 'Save changes', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
